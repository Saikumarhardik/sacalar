class HelloWorld {
    public static void main(String[] args) {
          String abc = "putup";
          String cba = "";
          
          for(int i = abc.length()-1;i>=0;i--){
              cba = cba + abc.charAt(i);
          }
          
          if(abc.equals(cba)){
              System.out.println("true");
          }
          else{
              System.out.println("false");
          }
          
    }
}
